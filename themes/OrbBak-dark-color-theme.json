	"semanticTokenColors": {
		"operatorOverload": "#e78a4e",
		"memberOperatorOverload": "#e78a4e",
		"class": {"foreground": "#90d7ff", "italic": true},
		"variable.defaultLibrary": "#d3869b",
		"property.defaultLibrary": "#d3869b",
		"interface": "#89b482",
		"enum": "#d3869b",
		"enumMember": "#7daea3",
		"namespace": "#d3869b",
		"class.builtin": {"foreground": "#00fdfd", "italic": true},
		"class.typeHint": {"foreground": "#0084ff", "italic": true},
		"enum:python":{"foreground": "#0282fa5e", "italic": true},
		"enumMember:python": "#1bb48ede",
		"intrinsic:python": "#d3869b",
		"module:python": "#ffa69e",
		"function": "#3fe41e",
		"function.decorator": "#89f08e",
		"parameter": "#B2CCD6",
		"property": "#cc96f0",
		"variable": "#7a96a3",
		"variable.readonly": "#696f8a",
		"method": "#699fdd",
		"builtinConstant": "#66ac6a",
		"builtinConstant.readonly": {"fontStyle": "italic"},
		"operator": "#c8ee70",
		"selfParameter": {"foreground":"#fd4dae", "italic": true},
		"*.declaration:python": {"fontStyle": "italic"},
		"macro:rust": "#89b482",
		"selfKeyword:rust": "#d3869b"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
			"comment", 
			"string.comment",
			"string.quoted.docstring",
			"string.quoted.docstring punctuation.definition.string.begin",
			"string.quoted.docstring punctuation.definition.string.end",
			"support.type.property-name.json.comments",
			"comment.line.number-sign.ini",
			"comment.line.number-sign.ignore",
			"string.quoted.docstring.multi",
		],
			"settings": {
				"foreground": "#31454e",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Punctuation Definition",
			"scope": [
				"punctuation.definition.comment",
				"punctuation.definition.string",
				"meta.fstring.python punctuation.definition.string string.quoted",
				"punctuation.definition.heading.markdown",
			],
			"settings": {
				"foreground": "#928374",
				"fontStyle": "italic"
			}
		},
		{
			"name": "F-String",
			"scope": [
				"meta.fstring.python storage.type.string",
			],
			"settings": {
				"foreground": "#f1b93e",
			}
		},
		{
			"name": "Item Access",
			"scope": [
				"meta.member.access meta.item-access",
				"meta.member.access meta.attribute",
			],
			"settings": {
				"foreground": "#85dfc4",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function Generic",
			"scope": "meta.function-call.generic",
			"settings": {
				"foreground": "#3FE41E"
			}
		},
		{
			"name": "Lambda",
			"scope": "storage.type.function.lambda",
			"settings": {
				"foreground": "#fa496f"
			}
		},
		{
			"name": "Decorator Definition",
			"scope": "meta.function.decorator",
			"settings": {
				"foreground": "#f7f46b"
			}
		},
		{
			"name": "Storage Type",
			"scope": "storage.type",
			"settings": {
				"foreground": "#f8ae1b"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted",
				"string.quoted.double.json.comments",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#f7d591"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.control.flow",
			],
			"settings": {
				"foreground": "#e65050",
			}
		},
		{
			"name": "Modificador",
			"scope": [
				"keyword.operator.logical",
			],
			"settings": {
				"foreground": "#e650d2",
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"meta.member.access meta.function-call.arguments",
				"meta.paragraph.markdown",
				"source"
			],
			"settings": {
				"foreground": "#b2ccd6"
			}
		},
		{
			"name": "Import",
			"scope": [
				"keyword.control.import",
		],
			"settings": {
				"foreground": "#a08241"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"keyword.operator.assignment",
				"keyword.operator.arithmetic",
				"keyword.operator.new",
				"keyword.operator.bitwise",
				"punctuation.separator",
				"rainbow1",
		],
			"settings": {
				"foreground": "#FF79C6"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#d16f37"
			}
		},
		// Add mas pode apagar
		{
			"scope": [
				"string constant.other.placeholder",
				"invalid.deprecated",
				"keyword",
			],
			"settings": {
				"foreground": "#D87818"
			}
		},
		{
			"scope": [
				"string constant.other.placeholder",
				"entity.name.function",
				"meta.definition.property.js",
				"meta.function-call.js",
				"punctuation.definition.interpolation.end.bracket.curly.scss",
				"punctuation.definition.interpolation.begin.bracket.curly.scss",
				"constant.language.json",
			],
			"settings": {
				"foreground": "#8bfc7c"
			}
		},
		{
			"scope": [
				"string",
				"string.quoted.double.json",
				"variable.other.constant.js",
				"variable.other.readwrite.js",
				"variable.other.readwrite.ts",
				"markup.fenced_code.block.markdown",
				"markup.inline.raw.string.markdown",
				"keyword.other.special-method",
				"markup.bold",
				"markup.bold.markdown",
				"markup.italic.markdown",
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor",
				"entity.name.tag.other.html",
				"entity.name.tag.block.any.html",
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#e9eff5b6",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"variable.parameter.function.language.special",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#f0c400"
			}
		},
		{
			"scope": [
				"entity.name.type"
			],
			"settings": {
				"foreground": "#d400ff"
			}
		},
		{
			"scope": [
				"invalid",
				"invalid.illegal",
				"meta.function.decorator meta.function-call.arguments"
			],
			"settings": {
				"foreground": "#18abd8"
			}
		},
	]
}